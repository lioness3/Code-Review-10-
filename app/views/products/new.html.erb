<div class="new_product_form">
  <%= form_for @products do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.integer :cost %>
  <%= f.text_field :cost %>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <%= f.collection_select(:product_id, @products, :id, :name)%>
  <%= f.submit  %>
  <% end %>
</div>


