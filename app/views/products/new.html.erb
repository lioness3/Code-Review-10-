<%= render "layouts/company_logo" %>

<%= render "layouts/errors", :object => @product %>
<h1>Publish a New Product </h1>
<div class="new_product_form">
  <%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label " $" %>
  <%= f.number_field(:cost, step: 0.01) %>
  <%= f.label "Made in:" %>
  <%= f.text_field :country_of_origin %>
  <%= f.submit  %>
  <% end %>
</div>


<%= render "layouts/company_info" %>
